{
  "name": "image-thumbnail-gen-backend",
  "version": "1.0.0",
  "description": "Image thumbnail generator server",
  "engines": {
    "node": "^14.19.0",
    "npm": "6.14.16"
  },
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run build && node ./dist/src/index.js",
    "dev": "tsnd --respawn --transpile-only --rs ./src/index.ts",
    "build": "tsc --outDir ./dist/ --target es2016 --esModuleInterop true --module commonjs ./src/index.ts",
    "test": "cross-env NODE_ENV=test jest --watch"
  },
  "keywords": [
    "thumbnail",
    "generator"
  ],
  "author": "Ernest Irabor",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "typescript": "^4.6.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^15.14.9",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8"
  }
}
